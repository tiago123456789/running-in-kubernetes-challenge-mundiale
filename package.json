{
  "name": "challenge_mundiale",
  "version": "1.0.0",
  "description": "Challenge mundiale implemented crawler",
  "main": "./src/index.js",
  "scripts": {
    "start": "npm run build && pm2 ./build/index.js",
    "build": "babel ./src --out-dir ./build",
    "start:dev": "nodemon --exec babel-node --presets=env ./src/index.js",
    "test:unit": "mocha --exit --colors --require @babel/register --require ./tests/config.js ./tests/unit//**/*.spec.js",
    "test:integration": "mocha --exit --colors --require @babel/register --require ./tests/config.js ./tests/integration/**/*.spec.js",
    "test": "npm run test:unit && npm run test:integration"
  },
  "author": "Tiago R. data costa",
  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "redis": "^3.0.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/register": "^7.9.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "mocha": "^7.1.2",
    "nodemon": "^2.0.4",
    "sinon": "^9.0.2",
    "supertest": "^4.0.2"
  }
}
